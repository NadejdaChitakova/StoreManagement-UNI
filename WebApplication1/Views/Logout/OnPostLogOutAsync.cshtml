@model WebApplication1.Controllers.LogoutController
<form method="post">
    <div class="container mt-5">
        <div class="row justify-content-center">
            <div class="col-sm-12 col-mg-12 col-lg-4">
                <p>Are you sure you want to logout?</p>
                <p>
                    <button class="btn btn-primary" asp-asp-page-handler="LogOut">Yes</button>
                    <button class="btn btn-light ml-5 border-dark" asp-asp-page-handler="DontLogOut">No</button>
                </p>
            </div>
        </div>
    </div>
</form>